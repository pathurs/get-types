{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true, "single"
        ],
        "interface-name": [
            true, "never-prefix"
        ],
        "ordered-imports": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "no-console": [
            false
        ],
        "arrow-parens": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}